<h1><%=@doctor.name%></h1>
<h3><%=@doctor.department%></h3>
<ul>
  <%@doctor.appointments.each do |appointment| %>
  <li> <%=appointment.appointment_datetime.strftime("%B %e, %Y at %H:%M")%></li>
  <li> <%=link_to appointment.patient.name, patient_path(appointment.patient)%> </li>
  <br></br>
  <%end %>
</ul>
